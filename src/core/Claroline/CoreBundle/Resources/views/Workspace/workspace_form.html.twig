{% extends "ClarolineCoreBundle:Workspace:layout.html.twig" %}
{% block section_content %}

<h4>{{ 'workspace.create'|trans({}, 'platformTranslation') }} : </h4>
{% form_theme form 'ClarolineCoreBundle::form_theme.html.twig' %}
<form novalidate="validate" action="{{ path('claro_workspace_create_workspace') }}" method="post" {{ form_enctype(form) }}>
    {{ form_errors(form) }}
    {{ form_widget(form) }}
    <input type="submit" formnovalidate="formnovalidate"  />
</form>

{% endblock %}