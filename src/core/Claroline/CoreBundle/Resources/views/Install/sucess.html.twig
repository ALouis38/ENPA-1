{% extends 'ClarolineCoreBundle:Install:layout.html.twig' %} 
{% block content %}
<div class="span3">
         <h3> Progression</h3>
        <ol class="nav nav-list">
         {%block naviguation %}        
            {%endblock %}
           </ol>
            <p>  Progression</p>
            <div class="progress">
              <div class="bar" style="width: 100%;"></div>
            </div>
    </div>
             <div class="span6">
                 <h3>Installation reussie</h3>
                  {%block data %}
                   <div class="alert alert-success">
                    <p> Installation réussie avec succès</p>
                </div>
                   <p> <a href="{{path('claro_index')}}" class="button"> Allez à l'accueil</a></p>
{%endblock data %}
        </div>
            <div class="span2">
                <h3> Astuces/Aides</h3>
                <div class="action">
                    Si vous avez des versions supérieures à celle du tableau ,cela ne peut aller que mieux pour vous.
                    </div>
              
                </div>
            <div class="span12">
              
                    {% if app.session.getFlashes() is defined %}
                <div class="alert alert-error">
        {% for key, flash in app.session.getFlashes() %}
        {{ flash }} <br />
        {% endfor %}
    </div>
                    {% endif %}
        </div>
  
   </div>

{% endblock %}

