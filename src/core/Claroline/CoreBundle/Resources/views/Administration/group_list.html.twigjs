{% twig_js name="group_list" %}

{% for group in groups %}
<tr class="row-group">
    <td align="center">{{ group.name }}</td>
    <td>
        <a href ="#"
            data-group-id="{{ group.getId() }}"
            id="link-delete-group-{{ group.id }}"
            class="link-delete-group">{{ 'delete'|trans({}, 'platform') }}
        </a>
    </td>
</tr>
{% endfor %}