{% extends "ClarolineCoreBundle:Administration:layout.html.twig" %}

{% block section_content %}

<table id="widget-table"  class="table table-striped table-bordered table-condensed">
<thead>
    <tr>
        <th>{{ 'name'|trans({}, 'platform') }}</th>
        <th>{{ 'is_workspace_visible'|trans({}, 'platform') }}</th>
        <th>{{ 'is_dashboard_visible'|trans({}, 'platform') }}</th>
    </tr>
</thead>
<tbody id="widget-table-body">
    {% for widget in widgets %}
    <tr>
        <th>{{ widget.getName() }}</th>
    </tr>
    {% endfor %}
</tbody>
</table>
{% endblock %}