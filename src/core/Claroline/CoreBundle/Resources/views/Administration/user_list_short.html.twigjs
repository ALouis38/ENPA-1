{% twig_js name="user_list_short" %}

{% for user in users %}
    <tr id="row-user-{{ user.getId() }}" class="row-user">
        <td>{{ user.id }}</td>
        <td>{{ user.username }}</td>
        <td>{{ user.lastname }}</td>
        <td>{{ user.firstname }}</td>
        <td>
             <input type="checkbox" value="{{ user.id }}" class="chk-user" align="center"></input>
        </td>
    </tr>
{% endfor %}