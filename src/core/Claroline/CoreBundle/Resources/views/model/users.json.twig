[
    {% for user in users %}
    {
        "id": "{{ user.getId() }}",
        "username": "{{ user.getUsername() }}",
        "lastname": "{{ user.getLastName() }}",
        "firstname": "{{ user.getFirstName() }}",
        "administrativeCode": "{{ user.getAdministrativeCode() }}",
        "mail": "{{ user.getMail() }}",
        "roles": "{% for role in  user.getOwnedRoles() %}{{role.getTranslationKey()|trans({},'platform')}}{% if not loop.last %} ,{% endif %}{% endfor %}"
    }
    {% if not loop.last %},{% endif %}
    {% endfor %}
]