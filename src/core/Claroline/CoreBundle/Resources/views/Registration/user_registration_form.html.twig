{% extends 'ClarolineCoreBundle::content_layout.html.twig' %}

{% form_theme form 'ClarolineCoreBundle::form_theme.html.twig' %}

{% block content %}

<h4>{{ 'register'|trans({}, 'platform') }}</h4>

<form id="user_registration_form" action="{{ path('claro_registration_register_user') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <input type="submit" />
</form>

{% endblock %}