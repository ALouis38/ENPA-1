{
{% for resourceType in resourceTypes %}
    "{{resourceType.getType()}}": {
        "delete": {
            "name": "delete",
            "route": "{{ path('claro_resource_delete', {'instanceId': '_instanceId'}) }}",
            "async": true
        },
        "properties": {
            "name": "properties",
            "route": "{{ path('claro_resource_properties_form', {'resourceId': '_resourceId'})}}",
            "async": true
        }
        {% for customAction in resourceType.getCustomActions() %}
        , "{{ customAction.getAction() }}": {
            "name": "{{ customAction.getAction() }}",
            "route": "{{ path('claro_resource_custom', {'resourceType': resourceType.getType(), 'action': customAction.getAction(), 'instanceId': '_instanceId' }) }}",
            "async": {{ customAction.isAsync() }}
        }
        {% endfor %}
    }
{% if loop.last != true %},{% endif %}
{% endfor %}
}