{% if currentFolder is not null %}
    <div class='current_folder' id='{{prefix}}_current_folder' data-key='{{currentFolder.id}}' data-type='{{currentFolder.getResourceType().getType()}}' data-resource-id='{{currentFolder.getResource().getId()}}'>current folder: {{currentFolder.name}}</div>
{% endif %}
{% for instance in resources %}
    <div id = '{{prefix}}_instance_{{instance.id}}'>
        {{instance.name}}
        <div id='resource_menu_{{instance.id}}' class='resource_menu' data-key='{{instance.id}}' data-type='{{instance.type}}' data-resource-id='{{instance.resource_id}}'>menu</div>
        {% if instance.type == 'directory' %}
            <a data-key='{{instance.id}}' class='link_navigate_instance' id='{{prefix}}_link_navigate_instance_{{instance.id}}' href="#">navigate</a>
        {% endif %}
    </div>
{% endfor %}