[
    {% for resource in resources %}
    {
        "title": "{{ resource.getResource().getName() }}",
        "key": "{{ resource.getId() }}",
        "instanceId": "{{ resource.getId() }}",
        "instanceCount": {{ resource.getResource().getInstanceCount() }},
        "shareType": "{{ resource.getResource().getShareType() }}",
        "resourceId": "{{resource.getResource().getId() }}",
        "type": "{{ resource.getResource().getResourceType().getType() }}",
        "workspaceId": "{{ resource.getWorkspace().getId() }}",
        "path" : "{{resource.getPath()}}",
        "dateCreation" : "{{ resource.getCreationDate()|date('m/d/Y') }}",
        "tooltip":
            "added by {{ resource.getCreator().getUsername() }} the {{ resource.getCreationDate()|date('m/d/Y') }}\ncreated by {{ resource.getResource().getCreator().getUsername() }} the {{ resource.getResource().getCreationDate()|date('m/d/Y') }}\nthis resource is {{ resource.getResource().getShareType(true) }}\npath is {{ resource.getPath() }} "
        {% if resource.getResourceType.getNavigable() == 1 %}
            ,"isFolder": true
            ,"isLazy": true
        {% endif %}
        {% if resource.getResourceType.getIcon() is not null %}
            ,"icon" : "{{resource.getResourceType.getIcon()}}"
        {% endif %}
    }
    {% if not loop.last %},{% endif %}
    {% endfor %}
]