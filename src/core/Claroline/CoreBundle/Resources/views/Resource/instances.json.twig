[
    {% for instance in instances %}
    {
        "title": "{{ instance.getName() }}",
        "key": "{{ instance.getId() }}",
        "instanceId": "{{ instance.getId() }}",
        "instanceCount": {{ instance.getResource().getInstanceCount() }},
        "shareType": "{{ instance.getResource().getShareType() }}",
        "resourceId": "{{ instance.getResource().getId() }}",
        "type": "{{ instance.getResource().getResourceType().getType() }}",
        "typeId": "{{ instance.getResource().getResourceType().getId() }}",
        "workspaceId": "{{ instance.getWorkspace().getId() }}",
        "path" : "{{ instance.getPath() }}",
        "dateInstanceCreation" : "{{ instance.getCreationDate()|date('m/d/Y') }}",
        "tooltip":
            "added by {{ instance.getCreator().getUsername() }} the {{ instance.getCreationDate()|date('m/d/Y') }}\ncreated by {{ instance.getResource().getCreator().getUsername() }} the {{ instance.getResource().getCreationDate()|date('m/d/Y') }}\nthis instance is {{ instance.getResource().getShareType(true) }}\npath is {{ instance.getPath() }} "
        {% if instance.getResourceType.getNavigable() == 1 %}
            ,"isFolder": true
            ,"isLazy": true
        {% endif %}
        {% if instance.getResourceType.getIcon() is not null %}
            ,"icon" : "{{ instance.getResourceType.getIcon() }}"
        {% endif %}
    }
    {% if not loop.last %},{% endif %}
    {% endfor %}
]