{% twig_js name="resource_thumbnail_template" %}

<div class="img-box">
    <input type="checkbox" value="{{ resource.id }}"/>
    <img src="{{ webRoot }}/{{ resource.large_icon }}" data-id="{{ resource.id }}" data-type="{{ resource.type }}"/>
</div>
<div class="name">{{ resource.name }}</div>
<div class="dropdown" href="#">menu</div>