[
    {% for resource in resources %}
    {
        "title": "{{ resource.getName() }}",
        "key": "{{ resource.getId() }}",
        "instanceCount": {{ resource.getInstanceCount() }},
        "shareType": "{{ resource.getShareType() }}",
        "resourceId": "{{resource.getId() }}",
        "type": "{{ resource.getResourceType().getType() }}",
        "tooltip":
            "added by created by {{ resource.getCreator().getUsername() }}\nthis resource is {{ resource.getShareType(true) }}\nthis resource is placed in\n{% for instance in resource.getResourceInstances %}the directory {{instance.getParent().getName()}} in the workspace {{instance.getWorkspace().getName()}}\n {% endfor %}"
        {% if resource.getResourceType.getNavigable() == 1 %}
            ,"isFolder": true
            ,"isLazy": true
        {% endif %}
    }
    {% if not loop.last %},{% endif %}
    {% endfor %}
]