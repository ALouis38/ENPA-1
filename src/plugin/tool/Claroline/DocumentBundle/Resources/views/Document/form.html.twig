{% extends "ClarolineCoreBundle::content_layout.html.twig" %}
{% block content %}

<form action="{{path('claro_document_upload')}}" method="post" {{ form_enctype(form) }}>
{{ form_errors(form) }}
{% set label_file %}{{ 'file'|trans({}, 'document') }}{% endset %}
{{ form_label(form.file, label_file ) }}
{{ form_widget(form.file) }}
{{ form_rest(form) }}
<input type="submit" />
</form>
{% endblock %}