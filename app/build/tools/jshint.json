{
    "browser": true,
    "globals": {
        "$": false,
        "_": false,
        "Claroline": false,
        "Routing": false,
        "Twig": false,
        "alert": false,
        "console": false,
        "confirm": false
    },
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "immed": true,
    "indent": 4,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "plusplus": false,
    "quotmark": true,
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": true,
    "maxparams": 4,
    "maxdepth": 5,
    "maxstatements": 30,
    "maxcomplexity": 10,
    "maxlen": 120,
    "white": true
}